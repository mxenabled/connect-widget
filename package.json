{
  "name": "connect-widget",
  "private": true,
  "description": "A simple ui library for React",
  "version": "0.0.1",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@kyper/button": "^4.0.0",
    "@kyper/icon": "^1.18.0",
    "@kyper/input": "^6.1.0",
    "@kyper/institutionlogo": "^3.1.0",
    "@kyper/messagebox": "^3.1.0",
    "@kyper/progressindicators": "^3.1.0",
    "@kyper/select": "^2.0.0",
    "@kyper/selectionbox": "^2.1.0",
    "@kyper/text": "^3.0.0",
    "@kyper/textarea": "^4.1.0",
    "@kyper/tokenprovider": "^4.0.0",
    "@kyper/userfeedback": "^2.0.0",
    "@kyper/utilityrow": "^2.1.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/node": "^22.1.0",
    "axios": "^1.7.3",
    "bowser": "^2.11.0",
    "date-fns": "^3.6.0",
    "gettext.js": "^2.0.2",
    "js-sha256": "^0.11.0",
    "numeral": "^2.0.6",
    "posthog-js": "^1.154.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux-observable": "^3.0.0-rc.2",
    "velocity-animate": "^1.5.2",
    "vite-plugin-dts": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@eslint/js": "^9.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "prettier": "3.3.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.3.4"
  }
}
